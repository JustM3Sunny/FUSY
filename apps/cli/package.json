{
  "name": "@fusy/cli",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "bin": {
    "fusy": "dist/index.js"
  },
  "dependencies": {
    "@fusy/config": "workspace:*",
    "@fusy/core": "workspace:*",
    "@fusy/memory": "workspace:*",
    "@fusy/providers": "workspace:*",
    "@fusy/shared": "workspace:*",
    "@fusy/telemetry": "workspace:*",
    "@fusy/tools": "workspace:*"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "tsx src/index.ts",
    "test": "vitest run --passWithNoTests",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  }
}
